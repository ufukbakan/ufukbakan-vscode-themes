{
  "name": "ufuk-bakan-color-themes",
  "displayName": "Ufuk Bakan Color Themes",
  "description": "Improved Vue and Snazzy Light color themes with Ayu file icons and Fluent product icons",
  "version": "2.0.2",
  "engines": {
    "vscode": "*"
  },
  "scripts": {
    "build": "vsce package",
    "vsls": "vsce ls --tree"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ufukbakan/ufukbakan-vscode-themes.git"
  },
  "publisher": "UfukBakan",
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Improved Snazzy Light",
        "uiTheme": "vs",
        "path": "./themes/Improved Snazzy Light-color-theme.json"
      },
      {
        "label": "Improved Vue Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/improved-vue-theme-color-theme.json"
      }
    ],
    "iconThemes": [
			{
				"id": "ayu",
				"label": "Ayu",
				"path": "./ayu-icons.json"
			}
		],
    "productIconThemes": [
			{
				"id": "fluent-icons",
				"label": "Fluent Icons",
				"path": "./themes/fluent-icons.json"
			}
		]
  },
  "devDependencies": {
    "@vscode/vsce": "^3.4.2"
  }
}
